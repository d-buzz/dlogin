{
  "name": "Dlogin",
  "description": "Hive wallet with unstoppable domain ability",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.js"]
  },
  "option_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
      "tabs",
      "<all_urls>"
  ]
}